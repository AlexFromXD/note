          "editorMode": "code",
          "format": "time_series",
          "metricColumn": "metric",
          "rawQuery": true,
          "rawSql": "WITH base AS (\n  SELECT\n    $__timeGroup(finished_at, $__interval) AS time,\n    EXTRACT(EPOCH FROM (finished_at - started_at))::double precision AS dur_sec\n  FROM public.task\n  WHERE finished_at IS NOT NULL\n    AND started_at IS NOT NULL\n    AND $__timeFilter(finished_at)\n    AND host IN (${host:sqlstring})\n    AND status IN (${status:sqlstring})\n)\nSELECT time, 'avg_sec' AS metric, AVG(dur_sec) AS value FROM base GROUP BY 1\nUNION ALL\nSELECT time, 'p50_sec' AS metric, PERCENTILE_CONT(0.5)  WITHIN GROUP (ORDER BY dur_sec) FROM base GROUP BY 1\nUNION ALL\nSELECT time, 'p95_sec' AS metric, PERCENTILE_CONT(0.95) WITHIN GROUP (ORDER BY dur_sec) FROM base GROUP BY 1\nUNION ALL\nSELECT time, 'p99_sec' AS metric, PERCENTILE_CONT(0.99) WITHIN GROUP (ORDER BY dur_sec) FROM base GROUP BY 1\nORDER BY time, metric;\n",
          "refId": "A",
          "sql": {
            "columns": [
              {
                "parameters": [],
                "type": "function"
              }
            ],
            "groupBy": [
              {
                "property": {
                  "type": "string"
                },
                "type": "groupBy"
              }
            ],
            "limit": 50
          },
          "timeColumn": "time",
          "valueColumn": "value"
        }
      ],
